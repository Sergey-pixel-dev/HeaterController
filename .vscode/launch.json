{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug STM32F446 (OpenOCD)",
            "type": "cortex-debug",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/Debug/STM32F446.elf",
            "servertype": "openocd",
            "device": "STM32F446RET6",
            "configFiles": [
                "interface/stlink.cfg",
                "target/stm32f4x.cfg"
            ],
            "runToEntryPoint": "main",
            "svdFile": "${workspaceFolder}/STM32F446.svd",
            "searchDir": [
                "/usr/share/openocd/scripts"
            ],
            "gdbPath": "/usr/bin/arm-none-eabi-gdb",
            "objdumpPath": "/usr/bin/arm-none-eabi-objdump",
            "nmPath": "/usr/bin/arm-none-eabi-nm",
            "overrideGDBServerStartedRegex": "Info : Listening on port",
            "showDevDebugOutput": "raw"
        }
    ]
}